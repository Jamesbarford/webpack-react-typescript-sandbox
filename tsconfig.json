{
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
		"noUnusedParameters": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext", "dom"],
    "sourceMap": true,
    "jsx": "react",
    "resolveJsonModule": true,
    "noEmit": true,
    "allowJs": false
  },
  "exclude": ["node_modules"]
}
