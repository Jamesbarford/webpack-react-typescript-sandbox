{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
		"noUnusedParameters": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext", "dom"],
    // "target": "es5",
    "sourceMap": true,
    "jsx": "react",
    "resolveJsonModule": true,
    "noEmit": true,
    "allowJs": false
  },
  "exclude": ["node_modules"]
}
