{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedLocals": true,
		"noUnusedParameters": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es2018", "dom"],
    "target": "es5",
    "sourceMap": true,
    "jsx": "react",
    "resolveJsonModule": true,
    "noEmit": true,
    "allowJs": false
  },
  "exclude": ["node_modules"]
}
